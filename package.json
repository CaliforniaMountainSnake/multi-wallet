{
    "name": "finance-app",
    "version": "1.0.0",
    "description": "Let's say you have money in multiple currencies: US dollars, euro, rubbles, bitcoin, etc. And you want to know, how much money do you have in total? Given that exchange rates are constantly changing... With this app you can add all your amounts of money in different currencies into the single table and see how much money you have in total. The total result can be presented in any currency you want.",
    "private": true,
    "scripts": {
        "build-dev": "webpack --config webpack.dev.js",
        "build-prod": "webpack --config webpack.prod.js",
        "build-prod-analyze-bundle": "webpack --config webpack.prod.analyze-bundle.js",
        "update_browserslist": "npx browserslist@latest --update-db",
        "check-types": "tsc --noEmit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git@github_CaliforniaMountainSnake:CaliforniaMountainSnake/multi-wallet.git"
    },
    "browserslist": [
        "defaults"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bootstrap": "^5.1.3",
        "core-js": "^3.21.1",
        "react": "^18.0.0",
        "react-bootstrap": "^2.3.1",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/plugin-proposal-throw-expressions": "^7.16.7",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@fullhuman/postcss-purgecss": "^4.1.3",
        "@types/react": "^18.0.6",
        "@types/react-dom": "^18.0.2",
        "babel-loader": "^8.2.4",
        "css-loader": "^6.7.1",
        "html-webpack-plugin": "^5.5.0",
        "postcss": "^8.4.13",
        "postcss-loader": "^6.2.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.3",
        "webpack": "^5.72.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "webpack-merge": "^5.8.0"
    }
}
